		<Unit filename="..\src\main.cpp">
			<Option target="Debug" />
			<Option target="Release" />
		</Unit>
		<Unit filename="..\test\testCounter.cpp">
			<Option target="tictactoe_test" />
		</Unit>
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
